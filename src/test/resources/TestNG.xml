<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Smoke">
<parameter name="BaseURL" value="https://qa.koel.app/"/>
    <test name="Smoke Test" preserve-order="false">
        <classes>
            <class name="BaseTest"/>
            <class name="LoginTests">
                <methods>
                    <include name="loginInvalidEmailValidPassword"/>
                </methods>
            </class>
            <class name="ProfileTests"/>
            <class name="ContextClick"/>
            <class name="Hover"/>
            <class name="countPlaylistSongs"/>
        </classes>
    </test>
</suite>